<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#539F56"
    android:backgroundTint="#539F56"
    tools:context=".activities.perfil.GestionUsuario">

        <TextView
            android:id="@+id/textCambiarContrasenaGesUs"
            android:layout_width="150dp"
            android:layout_height="20dp"
            android:text="@string/txtCambiarContrasena"
            android:textColor="#212121"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.183"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.627"
            tools:text="Cambiar contraseña:" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/btnCambiarContrasenaGesUs"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:contentDescription="@string/CambiarContraseña"
            app:backgroundTint="@color/teal_700"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.69"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.638"
            app:srcCompat="@android:drawable/ic_lock_idle_lock" />

        <Button
            android:id="@+id/btnInformeUsGesUs3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#19635C"
            android:text="@string/informeHoras"
            android:textAllCaps="false"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.074" />

        <Button
            android:id="@+id/btnVolverGesUs"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#19635C"
            android:text="@string/volver"
            android:textAllCaps="false"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.507"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.963" />

        <TextView
            android:id="@+id/textFechaNacimientoGesUs"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/fechaNacimineto"
            android:textColor="#212121"

            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.173"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.492"
            tools:text="Fecha de Nacimiento:" />

        <TextView
            android:id="@+id/textTelefonoGesUs"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/telefonoEmpresa"
            android:textColor="#212121"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.136"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.292"
            tools:text="Telefono:" />

        <TextView
            android:id="@+id/textApellidosGesUs"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/apelidosEmpleado"
            android:textColor="#212121"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.137"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.227"
            tools:text="Apellidos:" />

        <TextView
            android:id="@+id/textDireccionGesUs"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/direccionEmpleado"
            android:textColor="#212121"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.137"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.359"
            tools:text="Dirección:" />

        <Button
            android:id="@+id/btnEstablecerDatosGesUs"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:backgroundTint="#19635C"
            android:shadowColor="#303030"
            android:text="@string/establecerDatos"
            android:textAllCaps="false"
            android:textColor="@color/white"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.788" />

        <TextView
            android:id="@+id/textNombEmpleadoGesUs"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/nombreEmpleado"
            android:textColor="#212121"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.164"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.161"
            tools:text="Nombre Empleado:" />

        <EditText
            android:id="@+id/editNombEmpleadoGesUs"
            android:layout_width="150dp"
            android:layout_height="20dp"
            android:autofillHints=""
            android:background="#FFFFFF"
            android:backgroundTint="#FFFFFF"
            android:ems="10"
            android:hint="@string/nombreEmpleado"
            android:includeFontPadding="true"
            android:inputType="text"
            android:paddingStart="5dp"
            android:paddingEnd="5dp"
            android:textColorHint="#2E7D32"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.938"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.161"
            tools:ignore="TouchTargetSizeCheck" />

        <EditText
            android:id="@+id/editCorreoGesUs"
            android:layout_width="150dp"
            android:layout_height="20dp"
            android:autofillHints=""
            android:background="#FFFFFF"
            android:backgroundTint="#FFFFFF"
            android:ems="10"
            android:hint="@string/emailCrearcuenta"
            android:includeFontPadding="true"
            android:inputType="text|textEmailAddress"
            android:paddingStart="5dp"
            android:paddingEnd="5dp"
            android:textColorHint="#2E7D32"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.938"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.556"
            tools:ignore="TouchTargetSizeCheck" />

        <EditText
            android:id="@+id/editTelefonoGesUs"
            android:layout_width="150dp"
            android:layout_height="20dp"
            android:autofillHints=""
            android:background="#FFFFFF"
            android:backgroundTint="#FFFFFF"
            android:ems="10"
            android:hint="@string/phoneNumber"
            android:includeFontPadding="true"
            android:inputType="number|text"
            android:paddingStart="5dp"
            android:paddingEnd="5dp"
            android:textColorHint="#2E7D32"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.938"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.291"
            tools:ignore="TouchTargetSizeCheck" />

        <EditText
            android:id="@+id/editCentroDeTrabajoGesUs"
            android:layout_width="150dp"
            android:layout_height="20dp"
            android:autofillHints=""
            android:background="#FFFFFF"
            android:backgroundTint="#FFFFFF"
            android:ems="10"
            android:hint="@string/centroTrabajoHint"
            android:includeFontPadding="true"
            android:inputType="textPersonName"
            android:paddingStart="5dp"
            android:paddingEnd="5dp"
            android:textColorHint="#2E7D32"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.938"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.424"
            tools:ignore="TouchTargetSizeCheck" />

        <EditText
            android:id="@+id/editDireccionGesUs"
            android:layout_width="150dp"
            android:layout_height="20dp"
            android:autofillHints=""
            android:background="#FFFFFF"
            android:backgroundTint="#FFFFFF"
            android:ems="10"
            android:hint="@string/direccionHint"
            android:includeFontPadding="true"
            android:inputType="textPersonName"
            android:paddingStart="5dp"
            android:paddingEnd="5dp"
            android:textColorHint="#2E7D32"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.938"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.36"
            tools:ignore="TouchTargetSizeCheck" />

        <EditText
            android:id="@+id/editFechaNacimientoGesUs"
            android:layout_width="150dp"
            android:layout_height="20dp"
            android:autofillHints=""
            android:background="#FFFFFF"
            android:backgroundTint="#FFFFFF"
            android:ems="10"
            android:hint="@string/fechaNacimientoHint"
            android:includeFontPadding="true"
            android:inputType="datetime"
            android:paddingStart="5dp"
            android:paddingEnd="5dp"
            android:textColorHint="#2E7D32"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.938"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.49"
            tools:ignore="TouchTargetSizeCheck" />

        <EditText
            android:id="@+id/editApellidosGesUs"
            android:layout_width="150dp"
            android:layout_height="20dp"
            android:autofillHints=""
            android:background="#FFFFFF"
            android:backgroundTint="#FFFFFF"
            android:ems="10"
            android:hint="@string/apellidoHint"
            android:includeFontPadding="true"
            android:inputType="textPersonName"
            android:paddingStart="5dp"
            android:paddingEnd="5dp"
            android:textColorHint="#2E7D32"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.938"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.226"
            tools:ignore="TouchTargetSizeCheck" />

        <TextView
            android:id="@+id/textCorreoGesUs"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/txtCorreo"
            android:textColor="#212121"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.131"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.558"
            tools:text="Correo:" />

        <TextView
            android:id="@+id/textCentroTrabajoGesUs"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/centroDeTrabajo"
            android:textColor="#212121"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.162"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.426"
            tools:text="Centro de Trabajo:" />

        <TextView
            android:id="@+id/txtTitleCrearEmpleadoGesUs"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/Usuario"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.14"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.085" />

        <ImageView
            android:id="@+id/imageViewGesUs"
            android:layout_width="133dp"
            android:layout_height="71dp"
            android:contentDescription="@string/logo"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.942"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.024"
            app:srcCompat="@drawable/logo" />

        <Button
            android:id="@+id/btnEliminarUsGesUs"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:backgroundTint="#19635C"
            android:text="@string/eliminarUsGesUs"
            android:textAllCaps="false"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.49"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.872" />


</androidx.constraintlayout.widget.ConstraintLayout>